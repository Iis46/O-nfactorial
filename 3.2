def factorial(container, n):
      if n == 1:
            print(container)
            return
      for i in range(n):
            factorial(container, n - 1)
            if n % 2 == 0:
                  container[i], container[n-1] = container[n-1], container[i]
            else:
                  container[0], container[n - 1] = container[n - 1], container[0]
